{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "LICENSE": true,
        ".gitattributes": true,
        ".jsbeautifyrc": true,
        ".vscode": true
    },
    "cSpell.ignoreWords": [
      "afterbegin"
    ],
    "cSpell.words": [
      "Pangasinan",
      "accesskey",
      "beforeend",
      "flexbox",
      "howto",
      "iframe",
      "iframes",
      "longman",
      "xlink",
      "xmlhttprequest"
    ],
        // Specify paths/files to ignore.
    "cSpell.ignorePaths": [
        "node_modules",        // this will ignore anything the node_modules directory
        "vscode-extension",    //
        ".git",                // Ignore the .git directory
        "**/data/internal_libs/sources_data.js"
    ],
    "editor.fontSize": 17,

}